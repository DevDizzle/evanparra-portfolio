{
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "hosting": {
    "site": "evanparra-portfolio-8576-3a8bf",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/contact",
        "function": "contactForm"
      }
    ],
    "predeploy": [
      "npm run build"
    ],
    "redirects": [
      {
        "source": "/prompt-engineering",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/resume",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/consultant",
        "destination": "/",
        "type": 301
      }
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
