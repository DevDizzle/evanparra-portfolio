{
  "name": "evanparra-portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./src/index.css -o ./public/assets/tailwind.css --minify",
    "build:app": "esbuild src/main.jsx --bundle --minify --outfile=public/assets/app.js --format=iife --target=es2017",
    "build": "npm run build:css && npm run build:app"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "tailwindcss": "^3.4.4"
  }
}
